    "bcrypt": "^5.1.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "pg": "^8.12.0",
    "prisma": "^5.19.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  },
