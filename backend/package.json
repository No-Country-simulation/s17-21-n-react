{
  "name": "backend",
  "version": "0.0.1",
  "description": "backend",
  "repository": "https://github.com/No-Country-simulation/s17-21-n-react",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src index.ts -w --out-dir dist",
    "watch-dev": "nodemon --ignore dist/",
    "build": "swc src index.ts -d dist",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "prisma generate && prisma migrate deploy"
  },
  "devDependencies": {
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "concurrently": "8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "nodemon": "3.1.4",
    "swc": "1.0.11",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "^22.5.1",
